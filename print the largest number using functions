a=int(input("enter the 1st  numbers: "))
b=int(input("enter the 2nd numbers: "))
c=int(input("enter the 3rd numbers: "))
def largest_of_three(a,b,c):
  if a>b and a>c:
    return a
  elif b>c and b>c:
    return b
  else:
    return c
largest=largest_of_three(a,b,c)
print(largest," is the largest number.")
